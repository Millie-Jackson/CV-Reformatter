{
  "description": "Template 1 strict meta (enforce spacing/keep and list indents)",
  "margins_cm": { "top": 2.0, "right": 2.0, "bottom": 2.0, "left": 2.0 },
  "base_font": { "name": "Calibri", "size_pt": 10 },
  "paragraph": {
    "spacing_before_pt": 0,
    "spacing_after_pt": 6,
    "line_spacing_rule": "SINGLE",
    "line_spacing_value": null
  },
  "headings": {
    "H1": {
      "name": "Calibri", "size_pt": 12, "bold": true, "all_caps": true,
      "spacing_before_pt": 12, "spacing_after_pt": 6, "keep_with_next": true
    },
    "H2": {
      "name": "Calibri", "size_pt": 11, "bold": true, "all_caps": true,
      "spacing_before_pt": 10, "spacing_after_pt": 4, "keep_with_next": true
    },
    "H3": {
      "name": "Calibri", "size_pt": 10, "bold": true, "all_caps": true,
      "spacing_before_pt": 8, "spacing_after_pt": 2, "keep_with_next": true
    }
  },
  "title_block": {
    "apply": true,
    "lines": 2,
    "name": "Calibri",
    "size_pt": 12,
    "bold": true,
    "all_caps": true
  },
  "lists": {
    "apply": true,
    "indent_cm": 0.63,
    "hanging_indent_cm": 0.63,
    "spacing_before_pt": 0,
    "spacing_after_pt": 0
  }
}
